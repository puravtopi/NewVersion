        result = requirValidation("locationid");

        return result;
    }

    function requirValidation(id) {
        var result = true;
        var text = $("#" + id).val();
        if (text.length === 0) {
            $("#error-" + id).show();
            result = false;
        }
        else {
            $("#error-" + id).hide();
            result = true;
        }
        return result;
