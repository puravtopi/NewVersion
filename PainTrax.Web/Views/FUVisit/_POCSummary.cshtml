@model IEnumerable<PainTrax.Web.ViewModel.POCSumarryVM>

<br />

<div class="col-lg-12">
    <table class="table table-hover">
        <thead>
            <tr>

                <th scope="col">Date</th>
                <th scope="col">Body Part</th>
                <th scope="col">Heading</th>
                <th scope="col">Mcode</th>
               
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td scope="row">
                            @(item.PDate != null ? item.PDate.ToShortDateString() : "")
                        </td>
                        <td>@item.BodyPart</td>
                        <td>@item.Heading</td>
                        <td>@item.MCODE</td>
                      
               
                    </tr>
                }
            }
        </tbody>
    </table>
    <!-- End Table with hoverable rows -->

</div>
