<script>
    document.addEventListener('DOMContentLoaded', function () {
        const tabs = document.querySelectorAll('.nav-link');
        const button = document.getElementById('printButtonSOAP');

        // Function to show or hide button based on selected tab
        function handleTabClick(e) {
            const tabId = e.target.id;

            if (tabId === 'home-tab') {
                button.style.display = 'block'; // Show the button for specific tabs
            } else {
                button.style.display = 'none';  // Hide the button for others
            }
        }

        // Add click event listener to each tab
        tabs.forEach(tab => {
            tab.addEventListener('click', handleTabClick);
        });

        // Initial check in case the page loads with an active tab
        handleTabClick({ target: document.querySelector('.nav-link.active') });
    });
</script>