    function fnUpdateSidePanel() {

        var dos = $('#dos').val();
        var doa = $('#doa').val();
        var dob = $('#dob').val();

        var isDobValid = true; // 👈 flag

        // ---------- DOA vs DOS validation ----------
        if (dos && doa) {
            if (new Date(doa) > new Date(dos)) {
                alert('DOA cannot be greater than DOS');
                $('#doa').val('');
                $('#pdDOA').text('');
                return;
            }
        }

        // ---------- DOB validation ----------
        if (dob && dob.length === 10) {
            var birthDate = new Date(dob);
            var todayDate = new Date();
            todayDate.setHours(0, 0, 0, 0);

            var minDate = new Date("1900-01-01");

            if (birthDate < minDate) {
                if (!dobAlertShown) {
                    alert("DOB must be after 01/01/1900");
                    dobAlertShown = true;
                }
                isDobValid = false;
            }
            else if (birthDate > todayDate) {
                if (!dobAlertShown) {
                    alert("DOB cannot be future date");
                    dobAlertShown = true;
                }
                isDobValid = false;
            }
            else {
                dobAlertShown = false; // reset once valid
            }
        }

        // ---------- Existing UI logic ----------
        var fullname = $("#lname").val() + ' ' + $("#fname").val();
        $('#pdName').text(fullname);

        $('#pdCT').text($("#compensation option:selected").text());
        $('#pdLoc').text($("#locationid option:selected").text());

        if (dos) $('#pdDOS').text(fngetAGE(dos));
        if (doa) $('#pdDOA').text(fngetAGE(doa));

        $('#pdGender').text($("#gender option:selected").text());

        // ---------- Age calculation (ONLY if DOB is valid) ----------
        if (dob && dos && isDobValid) {
            var birthDate = new Date(dob);
            var refDate = new Date(dos);

            var age = refDate.getFullYear() - birthDate.getFullYear();
            var monthDiff = refDate.getMonth() - birthDate.getMonth();

            if (monthDiff < 0 || (monthDiff === 0 && refDate.getDate() < birthDate.getDate())) {
                age--;
            }

            // ✅ Age alert only when DOB is valid
            if (age > 90) {
                alert('Age should not be greater than 90 years');
            }

            $('#pdAge').text(age);
            $('#pdDOB').text(fngetAGE(dob));
        }
    }

    function fngetAGE(inputDate) {
        if (inputDate) {
            var date = new Date(inputDate);

            // Extract month, date, and year
            var month = (date.getMonth() + 1).toString().padStart(2, '0'); // Months are zero-based
            var day = date.getDate().toString().padStart(2, '0');
            var year = date.getFullYear();

            // Format date as MM/dd/yyyy
            var formattedDate = month + '/' + day + '/' + year;

            return formattedDate;
        }
        return '';
    }

