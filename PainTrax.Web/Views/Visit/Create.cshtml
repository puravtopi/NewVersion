    function requirValidation(id) {
        var result = true;
        var text = $("#" + id).val();

        if (text.length === 0 || text === "0") {
            $("#error-" + id).show();
            return false;
        }

        // ✅ Extra validation only for DOB
        if (id === "dob") {

            var selectedDate = new Date(text);   // yyyy-MM-dd format
            var today = new Date();

            var minDate = new Date("1900-01-01");

            if (selectedDate < minDate) {
                $("#error-" + id).text("DOB must be after 01/01/1900").show();
                return false;
            }

            if (selectedDate > today) {
                $("#error-" + id).text("DOB cannot be future date").show();
                return false;
            }
        }

        $("#error-" + id).hide();
        return true;
    }

